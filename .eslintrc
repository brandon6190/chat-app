{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:react/recommended",
		"plugin:jsx-a11y/recommended"
  ],
  "parser": "babel-eslint",
	"rules": {
    "react/prop-types": 0,
    "react/jsx-no-comment-textnodes": 0,
    "react/no-unescaped-entities": 0,
    "react/jsx-key": 0
	},
	"plugins": ["react", "import", "jsx-a11y"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
